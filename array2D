//การสร้าง Dynamic Arrray
#include <iostream>
#include <string>
using namespace std;
int main()
{   
	int num[3][2] = {3,1,5,4,6,8};
	cout<<num[0][0]<<endl<<"	"<<num[0][1]<<endl;
	cout<<num[1][0]<<endl<<"	"<<num[1][1]<<endl;
    cout<<num[2][0]<<endl<<"	"<<num[2][1]<<endl;
	cout<<"Get value from array 20 with For"<<endl;
	for(int i = 0;i<3;i++)
	{ 
		for(int j = 0;j<2;j++)
		{
			cout<<num[i][j]<<"\t";
		}
		cout<<endl;
	}
    system("pause");
    return 0;
}
